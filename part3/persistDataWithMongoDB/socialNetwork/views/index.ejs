<% include _header %>
    <h1>Welcome to the new social network! Learn about me!</h1>
    <% users.forEach((user) => { %>
        <div class="panel panel-default">
            <div class="panel-heading">
                <a href="/users/<%= user.username %>" >
                    <%= user.name() %>
                </a>
            </div>
            <% if(user.bio) { %>
                <div class="panel-body"><%= user.bio%></div>
            <% } %>
        </div>
    <% }) %>
<% include _footer %>